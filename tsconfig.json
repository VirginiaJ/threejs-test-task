{
  "compilerOptions": {
    "outDir": "commonjs",
    "target": "esNext",
    "baseUrl": "./",
    "lib": ["esNext", "dom"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "moduleResolution": "node"
  },
  "include": ["index.ts"],
  "exclude": ["node_modules"]
}
